<?xml version="1.0"?>
<package >
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <title>Client library for communicating to the Sonnen Portal API</title>
    <authors>Philip Daubmeier</authors>
    <owners>Philip Daubmeier</owners>
    <license type="file">LICENSE</license>
    <projectUrl>https://github.com/philipdaubmeier/GraphIoT/tree/master/src/SonnenClient</projectUrl>
