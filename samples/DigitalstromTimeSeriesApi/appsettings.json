{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "Network": {
    "Proxy": {
      "UseProxy": false,
      "Address": "127.0.0.1",
      "Port": 8080
    }
  },
  "ConnectionStrings": {
    "DigitalstromTimeSeriesDB": "<sql server connection string>"
  },
  "TokenStoreConfig": {
    "ClassNameMapping": {
      "PersistingDigitalstromAuth": "digitalstrom.timeseriesapi"
    }
  },
  "DigitalstromConfig": {
    "PollingService": {
      "Name": "Digitalstrom",
      "TimerInterval": "5m"
    },
    "TokenAppId": "DigitalstromTimeSeriesApi",
    "DssUsername": "<user>",
    "DssPassword": "<pass>",
    "DssCertificate": "MIIC+jCCA<...base64cert...>g==",
    "DssUriLocal": "https://10.0.0.1:8080",
    "DssUriDsNet": "https://{CloudDssId}.digitalstrom.net:8080/",
    "DssUriCloudredir": "https://cloudredir.somedomain.tld/?adr={CloudDssId}&token={CloudredirToken}&path=",
    "CloudDssId": "<dssId>",
    "CloudredirToken": "<redirToken>",
    "UseCloudredir": false
  }
}